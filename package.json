{
  "name": "ng-portfolio",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "build:analyze": "ng build --configuration analyze",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "analyze": "ng build --configuration analyze && npx webpack-bundle-analyzer dist/ng_portfolio/browser/stats.json",
    "lighthouse": "npx lighthouse http://localhost:4200 --output html --output-path ./lighthouse-report.html",
    "performance": "npm run build:prod && npm run lighthouse"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.2.0",
    "@angular/common": "^17.2.0",
    "@angular/compiler": "^17.2.0",
    "@angular/core": "^17.2.0",
    "@angular/forms": "^17.2.0",
    "@angular/platform-browser": "^17.2.0",
    "@angular/platform-browser-dynamic": "^17.2.0",
    "@angular/router": "^17.2.0",
    "@contentful/rich-text-html-renderer": "^16.3.5",
    "aos": "^2.3.4",
    "bootstrap": "^5.3.3",
    "contentful": "^10.10.0",
    "ng-bootstrap": "^0.46.0",
    "ng-count-up-js": "^1.1.1",
    "ng-portfolio": "file:",
    "ngx-bootstrap": "^12.0.0",
    "ngx-contentful-rich-text": "^0.9.0",
    "ngx-editor": "^17.5.4",
    "ngx-toastr": "^18.0.0",
    "ngx-typed-writer": "^1.2.0",
    "rxjs": "~7.8.0",
    "swiper": "^11.1.3",
    "tslib": "^2.3.0",
    "type-writer-xs": "^1.1.10",
    "typewriter-effect": "^2.21.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.2.0",
    "@angular/cli": "^17.2.0",
    "@angular/compiler-cli": "^17.2.0",
    "@types/aos": "^3.0.7",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lighthouse": "^11.7.1",
    "typescript": "~5.3.2",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
